{
    "name": "spryker-middleware/report",
    "description": "Report module",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "spryker-middleware/process": "dev-dev"
    },
    "require-dev": {
        "spryker/calculation": "*",
        "spryker/cart": "*",
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/silex": "*",
        "spryker/testify": "*",
        "spryker/transfer": "*"
    },
    "autoload": {
        "psr-4": {
          "SprykerMiddleware\\": "src/SprykerMiddleware/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/process.git"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/logger.git"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/report.git"
        }
    ]
}
